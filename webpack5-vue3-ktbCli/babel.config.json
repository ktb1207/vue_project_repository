{
  "presets": [
    [
      "@babel/env",
      {
        "targets": "> 0.25%, not dead",
        "corejs": "3",
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@vue/babel-plugin-jsx",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": "3"
      }
    ]
  ],
  "sourceMaps": true
}